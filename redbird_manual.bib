journal = {Breast Cancer Research and Treatment},
keywords = {Breast cancer,Diffuse optical imaging,Near infrared spectroscopy,Optical mammography,Optical tomography},
month = {mar},
number = {1},
pages = {9--22},
title = {{Diffuse optical imaging of the healthy and diseased breast: A systematic review}},
url = {http://link.springer.com/10.1007/s10549-007-9582-z},
volume = {108},
year = {2008}
}
