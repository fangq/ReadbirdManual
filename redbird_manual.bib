}
@misc{Pisano2005,
   abstract = {In digital mammography, the processes of image acquisition, display, and storage are separated, which allows optimization of each. Radiation transmitted through the breast is absorbed by an electronic detector, the response of which is faithful over a wide range of intensities. Once this information is recorded, it can be displayed by using computer image-processing techniques to allow arbitrary settings of image brightness and contrast, without the need for further exposure to the patient. In this article, the current state of the art in technology for digital mammography and data from clinical trials that support the use of the technology will be reviewed. In addition, several potentially useful applications that are being developed with digital mammography will be described. © RSNA, 2005.},
