   issue = {10},
   journal = {Biomedical Optics Express},
   month = {10},
   pages = {4275},
   publisher = {The Optical Society},
   title = {Functional imaging of the human brain using a modular, fibre-less, high-density diffuse optical tomography system},
   volume = {7},
   year = {2016},
}
